{
  "name": "youtube-downloader",
  "homepage": "./",
  "main": "./public/electron.js",
  "version": "1.0.0",
  "description": "Simple Electron app for downloading YouTube videos.",
  "private": true,
  "repository": "https://github.com/Julicolo/youtube-downloader",
  "author": "Julio Schilders <julio.schilders@gmail.com>",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1"
  },
  "scripts": {
    "dev": "concurrently \"yarn start\" \"wait-on http://localhost:3000 && yarn run electron\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "ebuild": "yarn run build && node_modules/.bin/build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "electron": "^6.0.9",
    "electron-builder": "^21.2.0",
    "electron-is-dev": "^1.1.0",
    "prettier": "^1.18.2",
    "wait-on": "^3.3.0"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": false
  }
}
